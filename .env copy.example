# OpenAI API Key - Required for GPT-4o Vision analysis
OPENAI_API_KEY=your_openai_api_key_here
# MongoDB connection string
MONGODB_URI=mongodb://mongodb:27017/pdfreviewer

# Max file size per PDF upload (in MB)
MAX_FILE_SIZE_MB=50

# Node environment
NODE_ENV=development

# Frontend API URL (used by Vite at build time)
VITE_API_URL=http://localhost:3001

# Google OAuth Client ID (from Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id_here

# JWT secret for signing session tokens (use a strong random value in production)
JWT_SECRET=your_jwt_secret_here

# Frontend Google Client ID (used by Vite at build time)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
