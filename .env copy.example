# OpenAI API Key - Required for GPT-4o Vision analysis
OPENAI_API_KEY=your_openai_api_key_here
# MongoDB connection string
MONGODB_URI=mongodb://mongodb:27017/pdfreviewer

# Max file size per PDF upload (in MB)
